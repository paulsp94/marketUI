    "boron": "0.2.3",
    "cheerio": "0.22.0",
    "compression": "1.6.1",
    "cross-env": "1.0.7",
    "cryptico": "1.0.2",
    "crypto-js": "3.1.6",
    "css-loader": "0.23.1",
    "ejs": "2.4.1",
    "express": "4.13.4",
    "express-ejs-layouts": "2.0.0",
    "firebase": "3.5.3",
    "firebase-admin": "4.0.4",
    "flexbox-react": "4.0.2",
    "flux": "2.1.1",
    "formsy-react": "0.18.1",
    "fs": "0.0.1-security",
